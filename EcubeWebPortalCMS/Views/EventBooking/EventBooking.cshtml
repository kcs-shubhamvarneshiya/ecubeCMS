                            @{
                                if (Model.EventAttachment != null && Model.EventAttachment != "")
                                {
                                    if (Model.EventAttachmentDisplay.ToLower().Contains("jpg") || Model.EventAttachmentDisplay.ToLower().Contains("png"))
                                    {
                                        <img id="AttachmentImage" alt="" onerror="this.onerror=null;this.src='/Content/Image/noprofile-img.jpg';" src="@Model.EventAttachment" class="movie-mobile--img" style="display: block;" />
                                    }
                                    if (Model.EventAttachmentDisplay.ToLower().Contains("xlsx"))
                                    {
                                        <img src="~/Content/Image/Excel_Icon.png" id="XlsFile" onerror="this.onerror=null;this.src='/Content/Image/noprofile-img.jpg';" style="width: 40px; margin: 5px 0; display: block;" />
                                    }
                                    if (Model.EventAttachmentDisplay.ToLower().Contains("pdf"))
                                    {
                                        <img src="~/Content/Image/Pdf_Icon.png" id="PDFfile" onerror="this.onerror=null;this.src='/Content/Image/noprofile-img.jpg';" style="width: 40px; margin: 5px 0; display: block;" />
                                    }
                                    if (Model.EventAttachmentDisplay.ToLower().Contains("docx"))
                                    {
                                        <img src="~/Content/Image/Document_Icon.png" id="DocFile" onerror="this.onerror=null;this.src='/Content/Image/noprofile-img.jpg';" style="width: 40px; margin: 5px 0; display: block;" />
                                    }

                                }
                                <img id="AttachmentImage" alt="" onerror="this.onerror=null;this.src='/Content/Image/noprofile-img.jpg';" src="@Model.EventAttachment" class="movie-mobile--img" style="display: none;" />
                                <img src="~/Content/Image/Pdf_Icon.png" id="PDFfile" onerror="this.onerror=null;this.src='/Content/Image/noprofile-img.jpg';" style="width: 40px; margin: 5px 0; display: none;" />
                                <img src="~/Content/Image/Document_Icon.png" id="DocFile" onerror="this.onerror=null;this.src='/Content/Image/noprofile-img.jpg';" style="width: 40px; margin: 5px 0; display: none;" />
                                <img src="~/Content/Image/Excel_Icon.png" id="XlsFile" onerror="this.onerror=null;this.src='/Content/Image/noprofile-img.jpg';" style="width: 40px; margin: 5px 0; display: none;" />
                                if (Model.Id > 0)
                                {
                                    if (@Model.EventAttachmentDisplay != null && @Model.EventAttachmentDisplay != "")
                                    {
                                        int index = @Model.EventAttachmentDisplay.IndexOf("?");
                                        if (index >= 0)
                                            @Model.EventAttachmentDisplay = @Model.EventAttachmentDisplay.Substring(0, index);
                                        <span id="lblEventAttachment">@Model.EventAttachmentDisplay</span>
                                    }
                                    else
                                    {
                                        <span id="lblEventAttachment">@Model.EventAttachment</span>
                                    }
                                }
                                else
                                {
                                    <span id="lblEventAttachment">@Model.EventAttachment</span>
                                }
                            }
                        </div>
                    </div>


                    <div class="col-md-4 mb-3 pb-3 d-block align-items-center" id="divCheckOutstadning">

                        @Html.CheckBoxFor(x => x.IsCheckOutstanding, new { @class = " form-control", @tabindex = "14" })
