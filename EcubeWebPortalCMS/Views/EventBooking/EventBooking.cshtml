                    <div class="col-4">
                        <input class="btn btn-inverse btn-primary" id="btnScheduleAdd" name="inputSubmit" type="button" value="Add Schedule" tabindex="30" title='Add Schedule' />
                    </div>
                </div>
                <div class=" user-contant">
                    @Html.Hidden("hfAddSchedule", @isAddSchedule)
                    @Html.Hidden("hfEditSchedule", @isEditSchedule)
                    @Html.Hidden("hfDeleteSchedule", @isDeleteSchedule)
                    <div id="dvEventSchedule">
