                        @Html.TextBoxFor(m => m.OutstandingAmountForMember, new { @class = "form-control", @tabindex = "15", @next = "ArtistsInfo" })*@
                    @*</div>*@


                    <div class="col-md-4 mb-3 pb-3">
                        <h5 class="mb-2">
                            Event Banner Image<span class="f_req"></span>
                        </h5>
                        <input type="file" id="EventBannerImage" tabindex="20" />
                        @Html.Hidden("hdnEventBannerImage", Model.EventBannerImage)
                        <div class="mt-2">
