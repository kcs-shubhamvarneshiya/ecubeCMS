@{
    ViewBag.Title = "Service Support";
    Layout = "~/Views/Shared/_Layout.cshtml";
    bool isAdd = Convert.ToBoolean(ViewData["blAddRights"]);
    bool isEdit = Convert.ToBoolean(ViewData["blEditRights"]);
    bool isDelete = Convert.ToBoolean(ViewData["blDeleteRights"]);
}
<div class="row-fluid">
    <div class="span12">
        <h4 class="heading">
            @ViewBag.Title
        </h4>
    </div>
    <div class="span12 clearfix contant-toph">
        @Html.Hidden("hfAdd", @isAdd)
        @Html.Hidden("hfEdit", @isEdit)
        @Html.Hidden("hfDelete", @isDelete)
        <div class="usearch-text">
            <label>
                Search
            </label>
            <input type="text" id="txtServiceSupportSearch" />
        </div>
    </div>
    <div class="span12 user-contant">
        <div id="dvServiceSupport">
            <table id="tblServiceSupport"></table>
            <div id="dvServiceSupportFooter"></div>
        </div>
    </div>
</div>
<script src="~/Scripts/Utility/ServiceSupportView.js"></script>
