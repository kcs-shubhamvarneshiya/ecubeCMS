                    </div>
                    <div class="col-md-4 mb-3 pb-3">
                        <h5 class="mb-2">
                            Event Duration  <span class="f_req"></span>
                        </h5>
                        @Html.TextBoxFor(m => m.EventDuration, new { @class = "  numwospc form-control ", @maxlength = "15", @tabindex = "6", @next = "GuestLimit" })
                    </div>
                    <div class="col-md-4 mb-3 pb-3">
                        <h5 class="mb-2">
                            Guest Limit <span class="f_req text-danger">*</span>
                        </h5>
                        @Html.TextBoxFor(m => m.GuestLimit, new { @class = "  numwospc form-control", @maxlength = "3", @tabindex = "7", @next = "EventStartTime" })
                    </div>
                    <div class="col-md-4 mb-3 pb-3">
                        <h5 class="mb-2">
                            Event Start Time
                        </h5>
                        @if (Model.Id > 0)
                        {
                            @Html.TextBoxFor(m => m.EventStartTime, new { @class = " required time form-control", @maxlength = "15", @tabindex = "8", @next = "EventEndTime", disabled = "disabled" })
                        }
