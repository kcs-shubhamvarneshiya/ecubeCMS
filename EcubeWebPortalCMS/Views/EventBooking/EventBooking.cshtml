                                @if (Model.Id > 0)
                                {
                                    @Html.RadioButtonFor(x => x.IsQRCode, "true", new { @class = "span5 form-control", @tabindex = "18", disabled = "disabled" })
                                }
                                else
                                {
                                    @Html.RadioButtonFor(x => x.IsQRCode, "true", new { @class = "span5 form-control", @tabindex = "18" })
                                }QR
                                @if (Model.Id > 0)
                                {
                                    @Html.RadioButtonFor(x => x.IsQRCode, "false", new { @class = "span5 form-control", @tabindex = "18", disabled = "disabled" })
                                }
                                else
                                {
                                    @Html.RadioButtonFor(x => x.IsQRCode, "false", new { @class = "span5 form-control", @tabindex = "18" })
                                }Barcode
                            </h5>
                        </div>
                    </div>
                    <div class="col-md-4 mb-3 pb-3">
                        <h5 class="mb-3">
                            Event Attachment<span class="f_req"></span>
                        </h5>
                        <input type="file" id="EventAttachment" tabindex="19" />
                        @Html.Hidden("hdnEventAttachment", Model.EventAttachment)
