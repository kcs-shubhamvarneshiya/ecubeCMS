                            <span class="pt-2 pb-2 d-block text-danger">Note: Allowed Format: .jpg, .gif, .png, .jpeg *image name should be image.jpg or image1.jpg</span>
                        </div>
                    </div>
                    <div class="col-md-4 mb-3 pb-3">
                        <h5 class="mb-2">
                            Event List Image<span class="f_req"></span>
                        </h5>
                        <input type="file" id="EventImage" tabindex="21" />
                        @Html.Hidden("hdnEventImage", Model.EventImage)
                        <div class="mt-2">
                            <img id="aEventImage" alt="" src="@Model.EventImage" onerror="this.onerror=null;this.src='/Content/Image/noprofile-img.jpg';" class="movie-mobile--img" />
                            <span class="pt-2 pb-2 d-block text-danger">Note: Allowed Format: .jpg, .gif, .png, .jpeg *image name should be image.jpg or image1.jpg </span>
                        </div>
                    </div>
                    <div class="col-md-4 mb-3 pb-3">
                        <h5 class="mb-2">
                            Mobile Banner Image<span class="f_req"></span>
                        </h5>
                        <input type="file" id="EventMobileImage" tabindex="22" />
                        @Html.Hidden("hdnEventMobileImage", Model.EventMobileImage)

                        <div class="mt-2">
                            <img id="aEventMobileImage" alt="" src="@Model.EventMobileImage" onerror="this.onerror=null;this.src='/Content/Image/noprofile-img.jpg';" class="movie-mobile--img" />
                            <span class="pt-2 pb-2 d-block text-danger">Note: Allowed Format: .jpg, .gif, .png, .jpeg *image name should be image.jpg or image1.jpg</span>
                        </div>
                    </div>

                    <div class="col-md-8 mb-3 pb-3">
                        <h5 class="mb-2">
                            Artists Information
                        </h5>
                        @Html.TextAreaFor(m => m.ArtistsInfo, new { @class = "  alphanumwithspc form-control ckeditor", @maxlength = "250", @tabindex = "23", @next = "EventSynopsis" })
                    </div>
                    <div class="col-md-8 mb-3 pb-3">
                        <h5 class="mb-2">
                            Event Synopsis
                        </h5>
                        @Html.TextAreaFor(m => m.EventSynopsis, new { @class = "  alphanumwithspc form-control ckeditor", @maxlength = "250", @tabindex = "24", @next = "TermsConditions" })
                    </div>
                    <div class="col-md-8 mb-3 pb-3">
                        <h5 class="mb-2">
                            Terms & Conditions
                        </h5>
                        @Html.TextAreaFor(m => m.TermsConditions, new { @class = "  alphanumwithspc form-control ckeditor", @maxlength = "250", @tabindex = "25", @next = "EventImage" })
                    </div>

                </div>
                <div class="d-flex justify-content-end mt-3">
                    <input class="btn btn-inverse btn-primary mr-2" id="btnEventSubmit" name="inputSubmit" type="button" value="Save & Next" tabindex="26" title='Save Event' />
                    <input class="btn btn-inverse btn-secondary Cancel" id="btnEventCancel" name="inputSubmit" type="button" value="Cancel" tabindex="27" title='Cancel' />
                </div>
            </div>
            @*<div class="">
                    <div class="">
                        <div class="d-flex justify-content-end mt-3">
                            <input class="btn btn-inverse btn-primary mr-2" id="btnEventSubmit" name="inputSubmit" type="button" value="Save & Next" tabindex="19" title='Save Event' />
                            <input class="btn btn-inverse btn-secondary Cancel" id="btnEventCancel" name="inputSubmit" type="button" value="Cancel" tabindex="20" title='Cancel' />
                        </div>
                    </div>
                </div>*@
        </div>
    </div>

    <div id="dvSchedule" class="panel panel-grey mt-5">
        <div class="d-flex align-items-center border-bottom pb-2 mb-3 main-form--heading ">
            <h4 class="heading mb-4 m-md-0">
                @ViewBag.Title Schedule
            </h4>
        </div>
        <h4>@ViewBag.Message</h4>
        <div class="panel-body pan">
            <div class="card shadow rounded overflow-hidden card-space">
                <div class="row d-flex align-items-end">
                    <div class="col-4">
                        <h5>
                            Event From Date<span class="f_req text-danger">*</span>
                        </h5>
                        @Html.HiddenFor(model => model.ObjEventScheduleModel.Id, new { @id = "hfdScheduleId" })
                        @Html.TextBoxFor(m => m.ObjEventScheduleModel.EventFromDate, new { @class = " dmmyyyyFromToDatePicker FromDatePicker form-control ", @maxlength = "15", @tabindex = "28", @next = "EventToDate" })

                    </div>
                    <div class="col-4">
                        <h5>
                            Event To Date<span class="f_req text-danger">*</span>
                        </h5>
                        @Html.TextBoxFor(m => m.ObjEventScheduleModel.EventToDate, new { @class = " dmmyyyyFromToDatePicker ToDatePicker form-control ", @maxlength = "15", @tabindex = "29", @next = "AvailableSeat" })
